/* atof example: sine calculator */
#include <stdio.h>      /* printf, fgets */
#include <stdlib.h>     /* atof */
#include <math.h>
#include <iostream>
int main ()
{
	std::cout << 16;
	int numBins = 16;

	int d_histogram[numBins];
	for (int i = 0 ; i < numBins ; i++){
		d_histogram[i] = 3*i;
	}
	int gid = 1;
	std::cout << d_histogram[gid];

	for (unsigned int s = 0 ; 2^s < numBins ; s++){
			if (s + gid < numBins){
				d_histogram[gid] += d_histogram[gid + 2^s];
			}
		}






	return 0;
}

main();
