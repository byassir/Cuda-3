# Add inputs and outputs from these tool invocations to the build variables 
CU_SRCS += \
../src/reduction.cu 

CU_DEPS += \
./src/reduction.d 

OBJS += \
./src/reduction.o 


# Each subdirectory must supply rules for building sources it contributes
src/%.o: ../src/%.cu
	@echo 'Building file: $<'
	@echo 'Invoking: NVCC Compiler'
	nvcc -I/usr/local/cuda-5.0/samples -I/usr/local/cuda/include -G -g -O0  -odir "src" -M -o "$(@:%.o=%.d)" "$<"
	nvcc --compile -G -I/usr/local/cuda-5.0/samples -I/usr/local/cuda/include -O0 -g  -x cu -o  "$@" "$<"
	@echo 'Finished building: $<'
	@echo ' '


