# Add inputs and outputs from these tool invocations to the build variables 
CPP_SRCS += \
../src/scrap.cpp 

OBJS += \
./src/scrap.o 

CPP_DEPS += \
./src/scrap.d 


# Each subdirectory must supply rules for building sources it contributes
src/%.o: ../src/%.cpp
	@echo 'Building file: $<'
	@echo 'Invoking: NVCC Compiler'
	nvcc -I/usr/local/cuda-5.0/samples -I/usr/local/cuda/include -G -g -O0  -odir "src" -M -o "$(@:%.o=%.d)" "$<"
	nvcc -I/usr/local/cuda-5.0/samples -I/usr/local/cuda/include -G -g -O0 --compile  -x c++ -o  "$@" "$<"
	@echo 'Finished building: $<'
	@echo ' '


